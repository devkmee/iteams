<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.iteams.pms.webhard.dao.WebHardDAO">
	
	<select id="selectWebHardList" resultType="kr.or.ddit.iteams.common.vo.AttachTotalVO" parameterType="string">
		SELECT ATT_NO,ATTACH_ORIGIN
		  FROM PMS_ATT
		 WHERE  ATTACH_NAME = #{attachName}
	</select>
</mapper>












